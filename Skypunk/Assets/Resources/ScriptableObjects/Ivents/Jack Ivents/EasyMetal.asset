  TextHeader: "\u0425\u0430\u043B\u044F\u0432\u043D\u044B\u0439 \u043C\u0435\u0442\u0430\u043B\u043B"
  Text: "\u0416\u0430\u043A \u0432\u044B\u0441\u0430\u0434\u0438\u043B\u0441\u044F
    \u043D\u0430 \u043F\u0443\u0441\u0442\u044B\u043D\u043D\u044B\u0439 \u043C\u0430\u043B\u0435\u043D\u044C\u043A\u0438\u0439
    \u043E\u0441\u0442\u0440\u043E\u0432, \u0433\u0434\u0435 \u043C\u043E\u0433 \u0443\u043C\u0435\u0441\u0442\u0438\u0442\u0441\u044F
    \u0442\u043E\u043B\u044C\u043A\u043E \u043E\u0434\u0438\u043D \u043C\u0430\u0433\u0430\u0437\u0438\u043D\u0447\u0438\u043A.
    \u0412 \u0432\u0438\u0442\u0440\u0438\u043D\u0435 \u043D\u0430 \u0442\u0430\u0431\u043B\u0438\u0447\u043A\u0435
    \u043A\u0440\u0430\u0441\u043E\u0432\u0430\u043B\u043E\u0441\u044C \u043A\u0440\u0443\u043F\u043D\u044B\u043C\u0438
    \u043A\u0440\u0438\u0432\u044B\u043C\u0438 \u0431\u0443\u043A\u0432\u0430\u043C\u0438
    \u043F\u0440\u0435\u0434\u043B\u043E\u0436\u0435\u043D\u0438\u0435 \u043D\u0435\u0432\u0438\u0434\u0430\u043D\u043D\u044B\u0445
    \u0441\u043A\u0438\u0434\u043E\u043A. \u0421\u0442\u043E\u0438\u0442 \u043B\u0438
    \u043E\u0442\u043A\u0430\u0437\u044B\u0432\u0430\u0442\u044C\u0441\u044F \u043E\u0442
    \u0442\u0430\u043A\u043E\u0439 \u0443\u0434\u0430\u0447\u0438?"
  VarA: "\u0417\u0430\u0439\u0442\u0438 \u0432 \u043C\u0430\u0433\u0430\u0437\u0438\u043D"
  TextA: "\u041A\u0430\u043A \u0442\u043E\u043B\u044C\u043A\u043E \u0416\u0430\u043A
    \u0441\u0442\u0443\u043F\u0438\u043B \u0432\u043D\u0443\u0442\u0440\u044C, \u0434\u0432\u0435\u0440\u0438
    \u0437\u0430 \u0435\u0433\u043E \u0441\u043F\u0438\u043D\u043E\u0439 \u0437\u0430\u0445\u043B\u043E\u043F\u043D\u0443\u043B\u0438\u0441\u044C.
    \u0417\u0430 \u0434\u0432\u0435\u0440\u044C\u044E \u0441\u043B\u044B\u0448\u0430\u043B\u0430\u0441\u044C
    \u0440\u0443\u0433\u0430\u043D\u044C \u0438 \u0437\u0432\u0443\u043A\u0438 \u0440\u0430\u0437\u0440\u0443\u0448\u0435\u043D\u0438\u0439.
    \u041F\u0440\u043E\u0448\u043B\u043E \u043D\u0435\u043C\u0430\u043B\u043E \u0432\u0440\u0435\u043C\u0435\u043D\u0438,
    \u043F\u0440\u0435\u0436\u0434\u0435 \u0447\u0435\u043C \u0416\u0430\u043A \u0441\u043C\u043E\u0433
    \u0432\u044B\u043B\u043E\u043C\u0430\u0442\u044C \u0434\u0432\u0435\u0440\u0438.
    \u041D\u043E \u043F\u0435\u0440\u0435\u0434 \u043D\u0438\u043C \u043F\u0440\u0435\u0434\u0441\u0442\u0430\u043B\u043E
    \u0432\u0435\u0441\u044C\u043C\u0430 \u043D\u0435\u043F\u0440\u0438\u044F\u0442\u043D\u043E\u0435
    \u0437\u0440\u0435\u043B\u0438\u0449\u0435 \u2013 \u0447\u0430\u0441\u0442\u044C
    \u043E\u0431\u0448\u0438\u0432\u043A\u0438 \u0435\u0433\u043E \u043A\u043E\u0440\u0430\u0431\u043B\u044F
    \u0440\u0430\u0437\u043E\u0431\u0440\u0430\u043B\u0438 \u043F\u0438\u0440\u0430\u0442\u044B"
